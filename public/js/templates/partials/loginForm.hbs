    <label>Your Email:</label>
    <input type="email" class="no-border form-control" id="email" name='email' placeholder="xxxx@---.com">
  </div>
  <div class="row form-row">
    <label>Password:</label>
    <input type="password" class="no-border form-control" id="password" name='password' placeholder="******">
  </div>
</form>
