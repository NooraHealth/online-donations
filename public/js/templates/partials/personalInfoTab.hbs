<div class="row">
  <div class="text-contain col-lg-2 col-sm-2 col-md-2 col-lg-offset-2 col-md-offset-2 col-sm-offset-2">
    <p><b>Name</b></p>
  </div>
  <div class="text-contain col-lg-4 col-sm-4 col-md-4">
    <p>{{metadata.name}}</p>
  </div>
</div>

<div class="row">
  <div class="text-contain col-lg-2 col-sm-2 col-md-2 col-lg-offset-2 col-md-offset-2 col-sm-offset-2">
    <p><b>Email</b></p>
  </div>
  <div class="text-contain col-lg-4 col-md-4 col-md-4">
    <p>{{email}}</p>
  </div>
  <div class="text-contain col-lg-2 col-sm-2 col-md-2">
    <p> <a id="change-email" class="text-contain silver skinny">Change Email</a> </p>
  </div>
</div>

<div class="row">
  <div class="text-contain col-lg-2 col-sm-2 col-md-2 col-lg-offset-2 col-md-offset-2 col-sm-offset-2">
    <p><b>Payment Info</b></p>
  </div>
  <div class="text-contain col-lg-4 col-sm-4 col-md-4">
    <p>{{getCardInfo this}}</p>
  </div>
  <div class="text-contain col-lg-2 col-sm-2 col-md-2">
    <p> <a id="change-payment-info" class="text-contain silver skinny">Change Payment Info</a> </p>
  </div>
</div>

<div class="row">
  <div class="text-contain col-lg-3 col-sm-3 col-md-3 col-lg-offset-2 col-md-offset-2 col-sm-offset-2">
    <p><a id="change-password" class="text-contain silver skinny">Change Password</a></p>
    <!--<button class="text-contain btn btn-default" id="change-password">Change Password</button>-->
  </div>
</div>
